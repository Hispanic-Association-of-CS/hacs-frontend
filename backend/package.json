{
  "name": "hacs-opensource-backend",
  "version": "1.0.0",
  "description": "Node, Express, MySQL Backend for HACS Website",
  "main": "app.js",
  "scripts": {
    "test": "jest --ci --verbose --forceExit --detectOpenHandles",
    "start:dev": "NODE_ENV=development DEBUG=APP_NAME:* nodemon ./bin/www",
    "start:windev": "SET \"NODE_ENV=development\" SET DEBUG=myapp:* & nodemon ./bin/www",
    "start": "NODE_ENV=production node ./bin/www",
    "db:create:migration": "sequelize migration:generate --name",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "db:create:model:announcements": "sequelize model:create --name announcements --attributes title:string,content:string,date:date",
    "db:create:model:officers": "sequelize model:create --name officers --attributes name:string,grade_level:string,description:string,email:string,phone_number:string,photo_link:string",
    "db:create:model:newsletter": "sequelize model:create --name newsletter --attributes title:string,pdf_link:string",
    "db:create:model:slides": "sequelize model:create --name slides --attributes title:string,pdf_link:string",
    "db:create:model:mentorship_program": "sequelize model:create --name mentorship_program --attributes mentor:string,mentee:string,point_total:integer",
    "db:create:model:sponsors": "sequelize model:create --name sponsors --attributes name:string,package_type:string,logo_link:string",
    "db:create:model:album": "sequelize model:create --name album --attributes name:string,description:string,album_size:integer",
    "db:create:model:pictures": "sequelize model:create --name pictures --attributes image_link:string,album_id:integer",
    "db:create:model:hacs_admin": "sequelize model:create --name hacs_admin --attributes name:string,email:string,password:string",
    "db:create:model:session": "sequelize model:create --name session --attributes sess:json",
    "db:seed:create": "sequelize seed:create --name",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo --seed",
    "db:seed:rollback": "sequelize db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-palos/hacs-opensource.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/j-palos/hacs-opensource/issues"
  },
  "homepage": "https://github.com/j-palos/hacs-opensource#readme",
  "dependencies": {
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-myconnection": "^1.0.4",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "promise-mysql": "^3.3.1",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "jest": "^24.1.0"
  }
}
